{
  "name": "google-assistant-hook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:client && /bin/bash scripts/build.sh",
    "build:client": "ng build --prod",
    "serve": "concurrently \"npm run serve:server\" \"npm run serve:client\"",
    "serve:server": "nodemon ./server/bin/www",
    "serve:client": "ng serve --proxy-config client/proxy.conf.json",
    "start": "NODE_ENV=production node ./server/bin/www",
    "test": "rm -rf ./coverage && npm run test:server && npm run test:client",
    "test:server": "nyc --reporter=lcov --report-dir=coverage/server --exclude=server/test/** mocha server/test/ --recursive --exit",
    "test:client": "ng test --watch=false --code-coverage",
    "coveralls": "cat ./coverage/client/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "format": "npm run format:server && npm run format:client",
    "format:server": "eslint server/** --fix",
    "format:client": "ng lint --fix && node ./scripts/format.js",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "eslint server/**",
    "lint:client": "ng lint",
    "e2e": "bash -x ./scripts/e2e.sh"
  },
  "nodemonConfig": {
    "ignore": [
      "server/test",
      "client",
      "log"
    ]
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "body-parser": "~1.15.1",
    "bootstrap": "4.0.0-beta.2",
    "bravia": "^1.2.0",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "feedparser": "^2.2.8",
    "firebase-admin": "^5.5.1",
    "google-home-notifier": "^1.2.0",
    "googleapis": "^23.0.0",
    "hbs": "^4.0.1",
    "http-status": "^1.0.1",
    "jquery": "^3.2.1",
    "log4js": "^2.4.1",
    "material-design-icons": "^3.0.1",
    "morgan": "~1.7.0",
    "popper.js": "^1.12.9",
    "request": "^2.83.0",
    "rxjs": "^5.5.2",
    "serve-favicon": "~2.3.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.1",
    "glob": "^7.1.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "js-beautify": "^1.7.5",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^4.1.0",
    "nock": "^9.1.6",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "protractor": "~5.1.2",
    "sinon": "^4.1.6",
    "supertest": "^3.0.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
